<% include estate_header %>
<div class="div-adv">
    <% include estate_menu %>
    <% include estate_adv %>
    <% include navigate %>

    <div class="statusbar">
        <span class="searchright">共<span class="red"> <%=total%> </span>个经纪人</span>
    </div>

    <%for(var i in record){%>
    	<div class="broker_div">
    		<div class="b_l">
    			<img src="/files/<%=record[i].img%>" alt="">
    		</div>
    		<div class="b_r">
    			<h1><%=record[i].name%></h1>
    			<h2><%=record[i].job%></h2>
    			<p>历史成交：<%=record[i].history%></p>
    			<p>总带看 ：<%=record[i].record%></p>
    			<p>所属门店：<%=record[i].store%></p>
    			<p>联系方式：<%=record[i].tel%></p>
    		</div>
    		<div class="clear"></div>
    		<hr/>
    		<p class="bd_p"><b>主营板块：</b><%=record[i].plate%></p>
    		<p class="bd_p"><b>重点小区：</b><%=record[i].keycell%></p>
    		<p class="bd_p"><b>经纪人自评：</b><%=record[i].selfrating%></p>
    		<br/>
    	</div>
    <%}%>
    <!--分页符-->
	<% include pager %> 
</div>
<script type="text/javascript">
function toPage(i){
   window.location = "/estate_broker?p="+i;
}
</script>
<% include estate_footer %>
